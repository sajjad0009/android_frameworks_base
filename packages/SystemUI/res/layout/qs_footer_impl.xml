<?xml version="1.0" encoding="utf-8"?>
<com.android.systemui.qs.QSFooterImpl
    android:id="@id/qs_footer"
    android:background="@android:color/transparent"
    android:clickable="false"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:layout_width="fill_parent"
    android:layout_height="@android:dimen/datepicker_selected_date_day_size"
    android:baselineAligned="false"
    android:elevation="4.0dip"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <FrameLayout
        android:layout_gravity="bottom|center"
        android:id="@id/qs_brightness_slider_parent"
        android:visibility="gone"
        android:layout_width="0.0dip"
        android:layout_height="42.0dip">
        <SeekBar
            android:id="@id/qs_brightness_slider"
            android:background="@null"
            android:paddingLeft="0.0px"
            android:paddingRight="0.0px"
            android:layout_width="fill_parent"
            android:layout_height="42.0dip"
            android:progressDrawable="@drawable/brightness_seekbar"
            android:thumb="@android:color/transparent" />
        <ImageView
            android:layout_gravity="center|left"
            android:layout_width="20.0dip"
            android:layout_height="20.0dip"
            android:layout_marginLeft="30.0dip"
            android:src="@drawable/brightness_left"
            android:scaleType="centerInside"
            android:tint="@color/brightness_slider_icons" />
        <ImageView
            android:layout_gravity="center|right"
            android:layout_width="20.0dip"
            android:layout_height="20.0dip"
            android:layout_marginRight="30.0dip"
            android:src="@drawable/brightness_right"
            android:scaleType="centerInside"
            android:tint="@color/brightness_slider_icons" />
    </FrameLayout>
    <LinearLayout
        android:gravity="end"
        android:layout_gravity="top"
        android:layout_width="fill_parent"
        android:layout_height="48.0dip"
        android:layout_marginStart="@dimen/qs_footer_margin"
        android:layout_marginEnd="@dimen/qs_footer_margin">
        <com.android.keyguard.AlphaOptimizedLinearLayout
            android:gravity="start|center"
            android:layout_gravity="start|center"
            android:id="@id/qs_footer_actions_edit_container"
            android:background="@drawable/abc_slice_remote_input_bg"
            android:layout_width="@integer/qs_footer_actions_width"
            android:layout_height="fill_parent"
            android:layout_marginTop="7.0dip"
            android:layout_marginBottom="7.0dip"
            android:layout_weight="@integer/qs_footer_actions_weight"
            android:layout_marginStart="10.0dip"
            android:layout_marginEnd="10.0dip">
            <com.android.systemui.statusbar.phone.MultiUserSwitch
                android:id="@id/multi_user_switch"
                android:background="@drawable/ripple_drawable"
                android:focusable="true"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_alignParentEnd="true">
                <ImageView
                    android:layout_gravity="center"
                    android:id="@id/multi_user_avatar"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:scaleType="centerInside" />
            </com.android.systemui.statusbar.phone.MultiUserSwitch>
            <TextView
                android:textAppearance="@style/TextAppearance.QS.Status"
                android:textSize="14.0sp"
                android:textColor="?android:textColorSecondary"
                android:gravity="center_vertical"
                android:id="@id/build"
                android:focusable="true"
                android:visibility="gone"
                android:clickable="true"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_marginStart="10.0dip" />
        </com.android.keyguard.AlphaOptimizedLinearLayout>
        <LinearLayout
            android:gravity="end"
            android:layout_width="0.0dip"
            android:layout_height="0.0dip">
            <com.android.systemui.qs.PageIndicator
                android:layout_gravity="center_vertical"
                android:id="@id/footer_page_indicator"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent" />
        </LinearLayout>
        <com.android.keyguard.AlphaOptimizedLinearLayout
            android:gravity="center"
            android:layout_gravity="start|center"
            android:id="@id/qs_footer_actions_container"
            android:background="@drawable/abc_slice_remote_input_bg"
            android:layout_width="@integer/qs_footer_actions_width"
            android:layout_height="fill_parent"
            android:layout_marginTop="7.0dip"
            android:layout_marginBottom="7.0dip"
            android:layout_weight="@integer/qs_footer_actions_weight">
            <com.android.systemui.statusbar.AlphaOptimizedImageView
                android:id="@android:id/edit"
                android:background="?android:selectableItemBackgroundBorderless"
                android:padding="@dimen/qs_footer_icon_padding"
                android:focusable="true"
                android:clickable="true"
                android:clipToPadding="false"
                android:layout_width="@dimen/qs_footer_action_button_size"
                android:layout_height="@dimen/qs_footer_action_button_size"
                android:src="@android:drawable/ic_menu_search_material"
                android:tint="?android:colorForeground"
                android:contentDescription="@string/accessibility_quick_settings_edit"
                android:layout_marginStart="5.0dip"
                android:layout_marginEnd="5.0dip" />
            <com.android.systemui.statusbar.AlphaOptimizedImageView
                android:id="@id/running_services_button"
                android:background="?android:selectableItemBackgroundBorderless"
                android:padding="@dimen/qs_footer_icon_padding"
                android:focusable="true"
                android:clickable="true"
                android:clipToPadding="false"
                android:layout_width="@dimen/qs_footer_action_button_size"
                android:layout_height="@dimen/qs_footer_action_button_size"
                android:src="@drawable/ic_settings_memory"
                android:tint="?android:colorForeground"
                android:contentDescription="@string/accessibility_quick_settings_running_services"
                android:layout_marginStart="5.0dip"
                android:layout_marginEnd="5.0dip"
                style="@android:style/Widget.Material.Button.Borderless" />
            <com.android.systemui.statusbar.AlphaOptimizedFrameLayout
                android:id="@id/settings_button_container"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:layout_width="@dimen/qs_footer_action_button_size"
                android:layout_height="@dimen/qs_footer_action_button_size"
                android:layout_marginStart="5.0dip"
                android:layout_marginEnd="5.0dip">
                <com.android.systemui.statusbar.phone.SettingsButton
                    android:layout_gravity="center"
                    android:id="@id/settings_button"
                    android:background="@drawable/ripple_drawable"
                    android:padding="@dimen/qs_footer_icon_padding"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:src="@drawable/ic_settings"
                    android:scaleType="centerInside"
                    android:tint="?android:colorForeground"
                    android:contentDescription="@string/accessibility_quick_settings_settings"
                    style="@android:style/Widget.Material.Button.Borderless" />
            </com.android.systemui.statusbar.AlphaOptimizedFrameLayout>
        </com.android.keyguard.AlphaOptimizedLinearLayout>
    </LinearLayout>
    <FrameLayout
        android:gravity="end"
        android:id="@id/qs_drag_handle_view"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="-5.0dip">
        <LinearLayout
            android:gravity="start"
            android:layout_gravity="top"
            android:background="@drawable/abc_slice_remote_input_bg"
            android:layout_width="160.0dip"
            android:layout_height="28.0dip"
            android:layout_marginTop="20.0dip"
            android:layout_marginStart="16.0dip"
            android:layout_marginEnd="16.0dip">
            <TextView
                android:textAppearance="@style/TextAppearance.QS.Status"
                android:textSize="15.0sp"
                android:ellipsize="marquee"
                android:gravity="center"
                android:layout_gravity="start|center"
                android:layout_width="85.0dip"
                android:layout_height="wrap_content"
                android:text="Data Used â€¢ "
                android:singleLine="true"
                android:marqueeRepeatLimit="marquee_forever" />
            <com.android.systemui.statusbar.info.DataUsageView
                android:textAppearance="@style/TextAppearance.QS.Status"
                android:gravity="center"
                android:layout_gravity="start|center"
                android:id="@id/data_sim_usage"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:singleLine="true" />
        </LinearLayout>
    </FrameLayout>
</com.android.systemui.qs.QSFooterImpl>